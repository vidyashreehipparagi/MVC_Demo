
@{
    ViewData["Title"] = "BookList";
}

<h1>BookList</h1>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Bid</th>
            <th>Bname</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Book b in (IEnumerable<Book>)ViewData["list"])
        {
            <tr>
                <td>@b.Bid</td>
                <td>@b.Bname</td>
                <td>@b.Price</td>
            </tr>
        }
        <tr>
            <td></td>
            <td></td>
            <td></td>
        </tr>
    </tbody>
</table>

